<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0"
            prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="monster.gameserver.Controller">
    <children>
        <Button fx:id="startBT" layoutX="50.0" layoutY="50.0" mnemonicParsing="false" onAction="#startServer"
                text="Start" AnchorPane.leftAnchor="50.0"/>
        <Button fx:id="StopBT" layoutX="200.0" layoutY="50.0" mnemonicParsing="false" onAction="#stopServer" text="Stop"
                AnchorPane.rightAnchor="50.0"/>
        <TextField fx:id="serverStatus" editable="false" layoutX="75.0" layoutY="215.0"/>
    </children>
</AnchorPane>
